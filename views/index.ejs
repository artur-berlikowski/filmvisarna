<!DOCTYPE html>
<html lang="en">

<head>
  <!-- META -->
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">

  <!-- CSS -->
  <link type="text/css" rel="stylesheet" href="/library/bootstrap-5.1.3-dist/css/bootstrap.min.css">
  <link type="text/css" rel="stylesheet" href="/library/bootstrap-5.1.3-dist/css/bootstrap-datepicker.min.css">
  <link type="text/css" rel="stylesheet" href="/css/default.css">

  <title>Filmvisarna</title>
</head>

<body>
  <navigation></navigation>
  <main>
    <% if (typeof ejs !=='undefined' ) { %>
      <% for (let view of ejs) { %>
        <%- include(view) %>
          <% } %>
            <% } %>
              <% if (typeof pages !=='undefined' ) { %>
                <% for (let page of pages) { %>
                  <%- include('./../public/html/' + page + '.html' ) %>
                    <% } %>
                      <% } %>
  </main>

  <script type="text/javascript" src="/library/jquery/jquery.js"></script>
  <script type="text/javascript" src="/library/popper/popper.min.js"></script>
  <script type="text/javascript" src="/library/bootstrap-5.1.3-dist/js/bootstrap.bundle.min.js"></script>
  <script type="text/javascript" src="/library/bootstrap-5.1.3-dist/js/bootstrap-datepicker.min.js"></script>
  <script type="text/javascript" src="/library/bootstrap-5.1.3-dist/js/locales/bootstrap-datepicker.sv.js"></script>

  <script type="text/javascript" src="/jsonflex.js"></script>
  <script type="text/javascript" src="/js/main.js"></script>

  <!-- page specific scripts -->
  <% if (typeof libs !=='undefined' ) { %>
    <% for (let lib of libs) { %>
      <script src='/js/<%= lib %>.js' type='text/javascript'></script>
      <% } %>
        <% } %>
          <!-- page specific modules -->
          <% if (typeof modules !=='undefined' ) { %>
            <% for (let module of modules) { %>
              <script type='module' src='/js/<%= module %>.js'></script>
              <% } %>
                <% } %>

          <!-- footer with links to other websites-->
          <footer>
            <div class="container-fluid">
              <div class="row">
                <div id="social-media-links" class="col">
                  <a class="facebook" href="https://www.facebook.com/"><img src="/image/facebook.png"
                      alt="Facebook"></a>
                  <a class="twitter" href="https://twitter.com/"><img src="/image/twitter.png" alt="Twitter"></a>
                  <a class="instagram" href="https://www.instagram.com/"><img src="/image/instagram.png"
                      alt="Instagram"></a>
                  <a class="linkedin" href="https://www.linkedin.com/"><img src="/image/linkedin.png"
                      alt="Linkedin"></a>
                </div>
              </div>
            </div>

                    <p id="copyright">Filmvisarna Â©2021.</p>
                    <script src="/js/footer.js"></script>
                  </footer>

</body>

</html>